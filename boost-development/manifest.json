{
  "manifest_version": 3,
  "name": "Content Script",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "*://*.www.wikipedia.org/*"
      ],
      "js": [
        "content-script/src/main.tsx"
      ],
      "media": []
    }
  ]
}